# Ass2
import java.util.*;
public class Main
{
    public static void main(String [] args)
    {
    Scanner sc=new Scanner(System.in);
    System.out.print("Enter value of a:");
    int a=sc.nextInt();
    System.out.print("Enter valuec of b:");
    int b=sc.nextInt();
    int sub=a+(~b)+1;
    System.out.print("The value of " + a + " - " + b + " is " + sub);
}
}
